PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX bsbm:<http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
SELECT *
WHERE {
    ?Product_6_2 rdfs:label ?ProductLabel_6_2 .
    ?Offer_6_2 bsbm:product ?Product_6_2 .
    ?Offer_6_2 bsbm:price ?Price_6_2 .
    ?Offer_6_2 bsbm:vendor ?Vendor_6_2 .
}

