con nombres de variables diferentes en onestop, y con las clausulas

q(X,U,Y,V) :- nationalflight(X,_0),fromla(U)
q(X,U,Y,V) :- nationalflight(X,_0),fromla(U)
q(X,U,Y,V) :- nationalflight(X,_0),frompa(U)
q(X,U,Y,V) :- nationalflight(X,_0),topa(X)
q(X,U,Y,V) :- nationalflight(X,_0),aaflight(X,U)
q(X,U,Y,V) :- nationalflight(X,_0),uaflight(X,U)
q(X,U,Y,V) :- nationalflight(X,_0),nationalflight(X,U)
q(X,U,Y,V) :- nationalflight(X,_0),onewayflight(X,U)
q(X,U,Y,V) :- nationalflight(_0,X),fromla(U)
q(X,U,Y,V) :- nationalflight(_0,X),fromla(U)
q(X,U,Y,V) :- nationalflight(_0,X),frompa(U)
q(X,U,Y,V) :- nationalflight(_0,X),topa(X)
q(X,U,Y,V) :- nationalflight(_0,X),aaflight(X,U)
q(X,U,Y,V) :- nationalflight(_0,X),uaflight(X,U)
q(X,U,Y,V) :- nationalflight(_0,X),nationalflight(X,U)
q(X,U,Y,V) :- nationalflight(_0,X),onewayflight(X,U)

